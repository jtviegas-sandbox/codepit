#%RAML 1.0 DataType
type: object
example: !include ../examples/newLanguage.json
properties:
  ISOCode:
    type: string
    description: A unique three letter ISO code for this language
    required: false
    examples:
      English: "eng"
      German: "ger"
  Name:
    type: string
    description: The name of the language in English.
    example: "GERMAN"
