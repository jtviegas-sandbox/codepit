#%RAML 1.0 DataType
type: object
example: !include ../examples/DocumentType.json
properties:
  Code:
    type: string
    description: |
      A unique code identifying which type, or the purpose of the attached document.
    example: "02"
  Description:
    type: string
    description: A text description of attached document type or purpose.
    example: "Termination"
